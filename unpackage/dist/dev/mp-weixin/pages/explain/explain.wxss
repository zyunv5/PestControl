	font-size: 0;
	background-color: rgb(234, 234, 234);
	padding: 20rpx 4% 20rpx 4%;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.explain-logo {
	width: 214rpx;
	height: auto;
	margin-bottom: 50rpx;
}
.explain-price1,
.explain-price2 {
	width: 653rpx;
	height: auto;
	margin: 0 auto 20rpx;
